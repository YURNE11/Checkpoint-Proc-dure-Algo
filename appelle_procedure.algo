PROCEDURE dot_product(v1 : ARRAY_OF INTEGER[n], v2 : ARRAY_OF INTEGER[n])
VAR
    i : INTEGER;
    ps : FLOAT;
BEGIN
    ps := 0.0;

    FOR i FROM 0 TO n - 1 DO
        ps := (ps + (v1[i] * v2[i]));
    END_FOR
END


ALGORITHM Produit_Scalaire
VAR
    PROCEDURE dot_product();
    i : INTEGER;
BEGIN
    N := 5;

    dot_product(ps);
    Write(ps);
END